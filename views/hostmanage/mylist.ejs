<html>

<head>
  <% include ./../header %>
</head>

<body>
  <% include ./../navbar %>
    <div class="container">

      <div class="row">
        <div class="col s8">
          <h1>My Items
            <a class="waves-effect waves-light btn" href="insert">Insert</a>
          </h1>
        </div>
      </div>

      <ul class="collapsible expandable">

        <% items.map(function(i) { %>
          <li>
            <div class="collapsible-header">
              <i class="material-icons">business</i><%= i.item_name%></div>
            <div class="collapsible-body">
              <div class="row">
                <div class="col s12">
                  
                </div>
              </div>
            </div>
          </li>
        <% }) %>
      </ul>
    </div>
</body>

<script>
  $(document).ready(function () {
    $('.collapsible').collapsible();
    $('.collapsible.expandable').collapsible({
    accordion: false
  });
  });
</script>

</html>